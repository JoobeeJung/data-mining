35188     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
35189     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
35191     +   /* Calculate sum of the squared entries for each row */
35192     +proc summary nway data=&transds;
35193     +   class &row;
35194     +   var &entry;
35195     +   output out=_sqrowvals uss=;
35196     +   run;
35198     +   /* Put into &meandiv what the average euclidean length is across rows */
35201     +%if &tmt_train = 1  %then %do;
35202     +   proc sql noprint;
35203     +      select mean(sqrt(&entry)) into :meaneuclen
35204     +      from _sqrowvals;
35205     +   quit;
35206     +   %if &tmt_config ne %then %do;
35207     +      *populate the config file with the mean value;
35208     +      data &tmt_config;
35209     +         set &tmt_config;
35210     +         &prefix._meaneuclen= symget('meaneuclen');
35211     +      run;
35212     +   %end;
35213     +    data _sqrowvals;
35214     +      set _sqrowvals;
35215     +      meaneuclen=symget('meaneuclen');
35216     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
35217     +      drop meaneuclen;
35218     +   run;
35221     +%end;
35222     +%else %do;
35223     +      * grab the mean value from the config file  and put into meaneuclien;
35224     +   data _null_;
35225     +      set &tmt_config;
35226     +      call symput('meaneuclen',&prefix._meaneuclen);
35227     +   run;
35228     +    data _sqrowvals;
35229     +      set _sqrowvals;
35230     +      meaneuclen=symget('meaneuclen');
35231     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
35232     +   run;
35234     +%end;
35239     +proc sql noprint;
35240     +   create table &outtransds as
35241     +      select a.&row,a.&col,a.&entry / divisor as &entry
35242     +      from &transds as a,_sqrowvals as b
35243     +      where a.&row=b.&row;
35244     +   drop table _sqrowvals;
35245     +         quit;
35246     +%if &col_sumds ne %then %do;
35247     +   proc summary nway data=&outtransds;
35248     +   class &col;
35249     +   var &entry;
35250     +   output out=&col_sumds mean=;
35251     +   run;
35252     +%end;
35253     +%mend row_pivot_normalize;
