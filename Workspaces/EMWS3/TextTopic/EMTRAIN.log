33660     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
33661     +         from &termds as b,_tmpTop as a
33662     +         where (b.key ne b.parent) and
33663     +         (a._term_ ne ' ' and a._role_ = ' ' and a._term_=b.term);
33664     +            quit;
33665     +   proc sql noprint;
33666     +      create table _termtop3  as
