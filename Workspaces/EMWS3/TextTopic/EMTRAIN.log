33471     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
33472     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
33474     +   /* Calculate sum of the squared entries for each row */
33475     +proc summary nway data=&transds;
33476     +   class &row;
33477     +   var &entry;
33478     +   output out=_sqrowvals uss=;
33479     +   run;
33481     +   /* Put into &meandiv what the average euclidean length is across rows */
33484     +%if &tmt_train = 1  %then %do;
33485     +   proc sql noprint;
33486     +      select mean(sqrt(&entry)) into :meaneuclen
33487     +      from _sqrowvals;
33488     +   quit;
33489     +   %if &tmt_config ne %then %do;
33490     +      *populate the config file with the mean value;
33491     +      data &tmt_config;
33492     +         set &tmt_config;
33493     +         &prefix._meaneuclen= symget('meaneuclen');
33494     +      run;
33495     +   %end;
33496     +    data _sqrowvals;
33497     +      set _sqrowvals;
33498     +      meaneuclen=symget('meaneuclen');
33499     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
33500     +      drop meaneuclen;
33501     +   run;
33504     +%end;
33505     +%else %do;
33506     +      * grab the mean value from the config file  and put into meaneuclien;
33507     +   data _null_;
33508     +      set &tmt_config;
33509     +      call symput('meaneuclen',&prefix._meaneuclen);
33510     +   run;
33511     +    data _sqrowvals;
33512     +      set _sqrowvals;
33513     +      meaneuclen=symget('meaneuclen');
33514     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
33515     +   run;
33517     +%end;
33522     +proc sql noprint;
33523     +   create table &outtransds as
33524     +      select a.&row,a.&col,a.&entry / divisor as &entry
33525     +      from &transds as a,_sqrowvals as b
33526     +      where a.&row=b.&row;
33527     +   drop table _sqrowvals;
33528     +         quit;
33529     +%if &col_sumds ne %then %do;
33530     +   proc summary nway data=&outtransds;
33531     +   class &col;
33532     +   var &entry;
33533     +   output out=&col_sumds mean=;
33534     +   run;
33535     +%end;
