33617     +   set &initds;
33618     +   /* Normalize data (terms all downcased), roles set as appropriate
33619     +    before output */
33620     +   _term_=klowcase(_term_);
33621     +   if propcase(_role_) in
33622     +      ("Adj","Adv","Aux","Conj","Det","Noun","Num","Part",
33623     +       "Prep", "Pron","Prop", "Verb")
33624     +      then _role_=propcase(_role_);
33625     +   if (_term_ ne ' ' or _role_ ne ' ') and _weight_ ne 0 and _weight_ ne . then output _tmpTop;
33626     +   run;
33627     +
33628     +    /* Now summarize all duplicates as mean of all the rows that are duplicated,
33629     +       for topic_cutoffs.
33630     +     */
33631     +   proc summary nway data=&topic_cutoff_ds;
33632     +   class _name;
33633     +   var _docCutoff _termCutoff;
