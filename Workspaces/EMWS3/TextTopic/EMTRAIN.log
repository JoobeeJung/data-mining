33669     +              when b.parent=. then b.key else b.parent end
33670     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
33671     +         from &termds as b,_tmpTop as a
33672     +         where (b.key ne b.parent) and
33673     +               (a._term_ ne ' ' and a._role_ ne ' ' and a._term_=b.term
33674     +                 and (a._role_=b.role or b.role=' '));
