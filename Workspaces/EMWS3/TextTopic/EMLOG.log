33638     +       with the topic_cutoff_ds to get term and document cutoffs */
33639     +   proc sql noprint;
33640     +      create table _tmptop as
33641     +         select a.*, b._doccutoff, b._termcutoff
33642     +         from _tmptop as a left join &topic_cutoff_ds as b
33643     +         on upcase(a._topic_)=upcase(b._name);
33644     +            quit;
33645     +
