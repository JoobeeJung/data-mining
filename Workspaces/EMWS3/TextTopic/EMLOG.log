33647     +      create table _termtop1  as
33648     +         select a._topic_,
33649     +            case
33650     +              when b.parent=. then b.key else b.parent end
33651     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
33652     +         from &termds as b,_tmpTop as a
33653     +         where (b.key ne b.parent) and (a._term_= ' ' and a._role_=b.role);
33654     +            quit;
33655     +   proc sql noprint;
33656     +      create table _termtop2  as
33657     +         select a._topic_,
33658     +            case
33659     +              when b.parent=. then b.key else b.parent end
33660     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
33661     +         from &termds as b,_tmpTop as a
33662     +         where (b.key ne b.parent) and
33663     +         (a._term_ ne ' ' and a._role_ = ' ' and a._term_=b.term);
33664     +            quit;
33665     +   proc sql noprint;
33666     +      create table _termtop3  as
33667     +         select a._topic_,
33668     +            case
33669     +              when b.parent=. then b.key else b.parent end
33670     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
33671     +         from &termds as b,_tmpTop as a
33672     +         where (b.key ne b.parent) and
33673     +               (a._term_ ne ' ' and a._role_ ne ' ' and a._term_=b.term
33674     +                 and (a._role_=b.role or b.role=' '));
33675     +            quit;
33676     +
33677     +
