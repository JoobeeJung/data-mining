25927     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
25928     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
25930     +   /* Calculate sum of the squared entries for each row */
25931     +proc summary nway data=&transds;
25932     +   class &row;
25933     +   var &entry;
25934     +   output out=_sqrowvals uss=;
25935     +   run;
25937     +   /* Put into &meandiv what the average euclidean length is across rows */
25940     +%if &tmt_train = 1  %then %do;
25941     +   proc sql noprint;
25942     +      select mean(sqrt(&entry)) into :meaneuclen
25943     +      from _sqrowvals;
25944     +   quit;
25945     +   %if &tmt_config ne %then %do;
25946     +      *populate the config file with the mean value;
25947     +      data &tmt_config;
25948     +         set &tmt_config;
25949     +         &prefix._meaneuclen= symget('meaneuclen');
25950     +      run;
25951     +   %end;
25952     +    data _sqrowvals;
25953     +      set _sqrowvals;
25954     +      meaneuclen=symget('meaneuclen');
25955     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
25956     +      drop meaneuclen;
25957     +   run;
25960     +%end;
25961     +%else %do;
25962     +      * grab the mean value from the config file  and put into meaneuclien;
25963     +   data _null_;
25964     +      set &tmt_config;
25965     +      call symput('meaneuclen',&prefix._meaneuclen);
25966     +   run;
25967     +    data _sqrowvals;
25968     +      set _sqrowvals;
25969     +      meaneuclen=symget('meaneuclen');
25970     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
25971     +   run;
25973     +%end;
25978     +proc sql noprint;
25979     +   create table &outtransds as
25980     +      select a.&row,a.&col,a.&entry / divisor as &entry
25981     +      from &transds as a,_sqrowvals as b
25982     +      where a.&row=b.&row;
25983     +   drop table _sqrowvals;
25984     +         quit;
25985     +%if &col_sumds ne %then %do;
25986     +   proc summary nway data=&outtransds;
25987     +   class &col;
25988     +   var &entry;
25989     +   output out=&col_sumds mean=;
25990     +   run;
25991     +%end;
25992     +%mend row_pivot_normalize;
