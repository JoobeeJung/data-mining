24259     +/* ****************************************************************
24260     + * Copyright (C) 2009 by SAS Institute Inc., Cary, NC 27513
24261     + *
24262     + * Name:             tmt_topify.sas
24263     + * Product:          SAS Text Miner
24264     + * Language:         Sas
24265     + * Script:
24266     + *
24267     + * Usage: %tmt_topify(initds=,termds=,topicds=,termtopicds=,<doccutoff=>);
24268     + *
24269     + * Purpose:  To convert a user-created table containing one row for
24270     + *      each term that contains a weight for each topic into a
24271     + *      normalized form with two tables :
24272     + *      a topic table with one row per topic, and a termtopics table
24273     + *      that has one row per term per topic.
24274     + *
24275     + * Parameters:
24276     + *   initds= The name of a table that contains one line per term per
24277     + * topic.  It must include the variables _topic_ (unique name of
24278     + * topic), _term_ (term text string), _role_ (part of speech or entity
24279     + * type).
24280     + *
24281     + *   termds= The name of a table that contains the terms matched up
24282     + * with their term ids, or key.  This table must include the variables
24283     + * key (the unique term id), term (term text string), role (part of
24284     + * speech or entity type), and parent (term id of parent if term
24285     + * represents a synonym of another term).
24286     + *
24287     + *   topicds= a table name that on output will contain one row per
24288     + * topic.  It contains the variables _topicid(unique identifier of
24289     + * topic, numbered sequentially beginning with 1), _name (unique name of
24290     + * topic), _cat (always set to "User" to indicate user topic), _apply
24291     + * (always set to Y so that topic will create a new variable on scored
24292     + * data representing topic), _doccutoff (set to input _docCutoff
24293     + * parameter), _termcutoff (set to zero), _numterms (set to missing to
24294     + * be calculated later), and _numdocs (set to missing to be calculated
24295     + * later)
24296     + *
24297     + *   topictermds= a table name that on output will contain one row for
24298     + * each term with a weight on each topic.  The variables on this table
24299     + * will be _topicid (unique id for each _topic as identified on
24300     + * topicds table), _termid (term ids as identified from the terms
24301     + * table for the term string and role string), and _weight (the weight
24302     + * to be applied to that term from the initds).
24303     + *
24304     + * History:
24305     + * 06May09 Initial Coding
24306     + *
24307     + * Notes:
24308     + *   The way that the term and role text strings are mapped into term
24309     + * ids via the terms data set obeys the following rules:
24310     + *
24311     + * 0. A normalized text string is created that is a downcased version
24312     + * of the term on the init_ds (since all terms are downcased on the
24313     + * terms table).  A normalized role is created in which roles
24314     + * representing parf of speech are set to have first letter
24315     + * uppercased, and the rest lowercased, again to match the term ds casing.
24316     +
24317     + * 1. If a given row on the initds contains both a non-blank term
24318     + * and role then a row is generated on termtopicds for each
24319     + * term on the term ds with that normalized text string and either
24320     + * that normalized role, or a blank role.
24321     + *
24322     + * 2. Any row on initds that has a blank role and a blank term is
24323     + * ignored.
24324     + *
24325     + * 3. Otherwise, any row that has a blank role matches terms in termds
24326     + * with any role.
24327     + *
24328     + * 4. Otherwise, any row with a blank term matches any terms in termds
24329     + * with the given role.
24330     + *
24331     + * Last Modified By:
24332     + * Last Modified On: Tue May 29 14:19:57 2012
24333     + *
24334     + * End
24335     + * ************************************************************** */
24336     +%macro tmt_topify(initds=,termds=,topicds=,termtopicds=,topic_cutoff_ds=,
24337     +                  doccutoff=.001,termcutoff=.001);
24338     +   data _tmptop (keep=_topic_ _term_ _role_ _weight_);
24339     +   set &initds;
24340     +   /* Normalize data (terms all downcased), roles set as appropriate
24341     +    before output */
24342     +   _term_=klowcase(_term_);
24343     +   if propcase(_role_) in
24344     +      ("Adj","Adv","Aux","Conj","Det","Noun","Num","Part",
24345     +       "Prep", "Pron","Prop", "Verb")
24346     +      then _role_=propcase(_role_);
24347     +   if (_term_ ne ' ' or _role_ ne ' ') and _weight_ ne 0 and _weight_ ne . then output _tmpTop;
24348     +   run;
24349     +
24350     +    /* Now summarize all duplicates as mean of all the rows that are duplicated,
24351     +       for topic_cutoffs.
24352     +     */
24353     +   proc summary nway data=&topic_cutoff_ds;
24354     +   class _name;
24355     +   var _docCutoff _termCutoff;
24356     +   output out=&topic_cutoff_ds mean=;
24357     +
24358     +
24359     +   /* Make sure to eliminate duplicates, and to roll children into parents.  Also join
24360     +       with the topic_cutoff_ds to get term and document cutoffs */
24361     +   proc sql noprint;
24362     +      create table _tmptop as
24363     +         select a.*, b._doccutoff, b._termcutoff
24364     +         from _tmptop as a left join &topic_cutoff_ds as b
24365     +         on upcase(a._topic_)=upcase(b._name);
24366     +            quit;
24367     +
24368     +   proc sql noprint;
24369     +      create table _termtop1  as
24370     +         select a._topic_,
24371     +            case
24372     +              when b.parent=. then b.key else b.parent end
24373     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
24374     +         from &termds as b,_tmpTop as a
24375     +         where (b.key ne b.parent) and (a._term_= ' ' and a._role_=b.role);
24376     +            quit;
24377     +   proc sql noprint;
24378     +      create table _termtop2  as
24379     +         select a._topic_,
24380     +            case
24381     +              when b.parent=. then b.key else b.parent end
24382     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
24383     +         from &termds as b,_tmpTop as a
24384     +         where (b.key ne b.parent) and
24385     +         (a._term_ ne ' ' and a._role_ = ' ' and a._term_=b.term);
24386     +            quit;
24387     +   proc sql noprint;
24388     +      create table _termtop3  as
24389     +         select a._topic_,
24390     +            case
24391     +              when b.parent=. then b.key else b.parent end
24392     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
24393     +         from &termds as b,_tmpTop as a
24394     +         where (b.key ne b.parent) and
24395     +               (a._term_ ne ' ' and a._role_ ne ' ' and a._term_=b.term
24396     +                 and (a._role_=b.role or b.role=' '));
24397     +            quit;
24398     +
24399     +
