24193     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
24194     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
24196     +   /* Calculate sum of the squared entries for each row */
24197     +proc summary nway data=&transds;
24198     +   class &row;
24199     +   var &entry;
24200     +   output out=_sqrowvals uss=;
24201     +   run;
24203     +   /* Put into &meandiv what the average euclidean length is across rows */
24206     +%if &tmt_train = 1  %then %do;
24207     +   proc sql noprint;
24208     +      select mean(sqrt(&entry)) into :meaneuclen
24209     +      from _sqrowvals;
24210     +   quit;
24211     +   %if &tmt_config ne %then %do;
24212     +      *populate the config file with the mean value;
24213     +      data &tmt_config;
24214     +         set &tmt_config;
24215     +         &prefix._meaneuclen= symget('meaneuclen');
24216     +      run;
24217     +   %end;
24218     +    data _sqrowvals;
24219     +      set _sqrowvals;
24220     +      meaneuclen=symget('meaneuclen');
24221     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
24222     +      drop meaneuclen;
24223     +   run;
24226     +%end;
24227     +%else %do;
24228     +      * grab the mean value from the config file  and put into meaneuclien;
24229     +   data _null_;
24230     +      set &tmt_config;
24231     +      call symput('meaneuclen',&prefix._meaneuclen);
24232     +   run;
24233     +    data _sqrowvals;
24234     +      set _sqrowvals;
24235     +      meaneuclen=symget('meaneuclen');
24236     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
24237     +   run;
24239     +%end;
24244     +proc sql noprint;
24245     +   create table &outtransds as
24246     +      select a.&row,a.&col,a.&entry / divisor as &entry
24247     +      from &transds as a,_sqrowvals as b
24248     +      where a.&row=b.&row;
24249     +   drop table _sqrowvals;
24250     +         quit;
24251     +%if &col_sumds ne %then %do;
24252     +   proc summary nway data=&outtransds;
24253     +   class &col;
24254     +   var &entry;
24255     +   output out=&col_sumds mean=;
24256     +   run;
24257     +%end;
24258     +%mend row_pivot_normalize;
