25910     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
25911     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
25913     +   /* Calculate sum of the squared entries for each row */
25914     +proc summary nway data=&transds;
25915     +   class &row;
25916     +   var &entry;
25917     +   output out=_sqrowvals uss=;
25918     +   run;
25920     +   /* Put into &meandiv what the average euclidean length is across rows */
25923     +%if &tmt_train = 1  %then %do;
25924     +   proc sql noprint;
25925     +      select mean(sqrt(&entry)) into :meaneuclen
25926     +      from _sqrowvals;
25927     +   quit;
25928     +   %if &tmt_config ne %then %do;
25929     +      *populate the config file with the mean value;
25930     +      data &tmt_config;
25931     +         set &tmt_config;
25932     +         &prefix._meaneuclen= symget('meaneuclen');
25933     +      run;
25934     +   %end;
25935     +    data _sqrowvals;
25936     +      set _sqrowvals;
25937     +      meaneuclen=symget('meaneuclen');
25938     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
25939     +      drop meaneuclen;
25940     +   run;
25943     +%end;
25944     +%else %do;
25945     +      * grab the mean value from the config file  and put into meaneuclien;
25946     +   data _null_;
25947     +      set &tmt_config;
25948     +      call symput('meaneuclen',&prefix._meaneuclen);
25949     +   run;
25950     +    data _sqrowvals;
25951     +      set _sqrowvals;
25952     +      meaneuclen=symget('meaneuclen');
25953     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
25954     +   run;
25956     +%end;
25961     +proc sql noprint;
25962     +   create table &outtransds as
25963     +      select a.&row,a.&col,a.&entry / divisor as &entry
25964     +      from &transds as a,_sqrowvals as b
25965     +      where a.&row=b.&row;
25966     +   drop table _sqrowvals;
25967     +         quit;
