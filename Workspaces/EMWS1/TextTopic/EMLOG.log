24403     +   proc sort data=&termtopicds; by _topic_;
24404     +
24405     +   /* Now create the topic data set, which has one row per topic, and
24406     +    the convert the termtopic data set to have one row per actual term
24407     +    per topic */
24408     +   data &topicds (keep=_topicid _name _displayCat _cat _docCutoff _termCutoff
24409     +                  _numterms _numdocs)
24410     +      &termtopicds (keep=_topicid _termid _weight);
